{
    "name": "monokai-dark",
    "displayName": "Monokai Dark",
    "version": "0.1.0",
    "publisher": "Delyan Dobrev",
    "engines": {
        "vscode": "*"
    },
    "categories": [
        "Themes",
        "Programming Languages"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Monokai Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/monokai-dark-color-theme.json"
            }
        ],
        "languages": [
            {
                "id": "rust",
                "aliases": [
                    "Rust",
                    "rust"
                ],
                "extensions": [
                    ".rs"
                ],
                "configuration": "./config/rust.json"
            }
        ],
        "grammars": [
            {
                "language": "rust",
                "scopeName": "source.rust",
                "path": "./syntaxes/Rust.tmLanguage.json"
            }
        ]
    }
}
